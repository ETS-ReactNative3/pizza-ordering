var _interopRequireDefault = require("@babel/runtime/helpers/interopRequireDefault");

var _react = _interopRequireDefault(require("react"));

var _enzyme = require("enzyme");

var _HomeScreen = _interopRequireDefault(require("./../HomeScreen.js"));

describe('Home Screen Test', function () {
  it('should render snapshot test for home screen', function () {
    var wrapper = (0, _enzyme.mount)(_react.default.createElement(_HomeScreen.default, null));
    expect(wrapper).toMatchSnapshot();
  });
  it('should navigate to Category Screen', function () {
    var props = {
      navigation: {
        navigate: jest.fn()
      }
    };
    var wrapper = (0, _enzyme.mount)(_react.default.createElement(_HomeScreen.default, props));
    wrapper.find('TouchableOpacity').simulate('press');
    expect(props.navigation.navigate).toBeCalledWith('Category Screen');
  });
});
